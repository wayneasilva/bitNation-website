<div class="container">
    <% articles.forEach(function(article) { %>
        <div class="box">
            <div class="container">
                <div class="title"><%= article.title %></div>
                <figure class="image is-64x64">
                    <img src="<%= article.image %>">
                </figure>
                <div class="container">
                    <%= article.body %>
                </div>
            </div>
        </div>
    <% }) %>
</div>

